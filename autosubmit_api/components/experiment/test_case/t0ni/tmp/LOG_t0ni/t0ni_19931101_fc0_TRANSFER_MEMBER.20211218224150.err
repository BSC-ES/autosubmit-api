      do_transfer "${remote_folder}/restarts/${type}_${EXPID}_${START_date}_${MEMBER}_${chunk_zero_padded}_${Chunk_start_date}-${Chunk_end_date}.tar*" ${destination_folder}/${START_date}/${MEMBER}/restarts
      ;;
